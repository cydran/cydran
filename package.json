{
  "name": "cydran",
  "version": "0.0.24",
  "description": "Unobtrusive Javascript presentation framework",
  "main": "dist/cydran.js",
  "types": "dist/cydran.d.ts",
  "scripts": {
    "start": "npm run clean && webpack -w --mode development",
    "build": "npm run clean && webpack --mode production && rimraf dist/src",
    "releaseBuild": "npm run lint && npm run build && npm run test && npm run build && npm run minify && npm run docs",
    "clean": "rimraf dist",
    "test": "webpack --config webpack-test.config.js",
    "lint": "tslint --project .",
    "minify": "uglifyjs dist/cydran.js --keep-fnames -o dist/cydran.min.js",
    "docs": "rimraf dist/docs && typedoc --mode modules --excludeExternals --excludePrivate --exclude '**/*+(logger|decorator|messaging)/**/*.ts' --out dist/docs src"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/cydran/cydran.git"
  },
  "keywords": [
    "MVVM",
    "components",
    "component",
    "framework"
  ],
  "author": "The Cydran team",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/cydran/cydran/issues"
  },
  "homepage": "https://github.com/cydran/cydran",
  "devDependencies": {
    "@babel/core": "7.6.2",
    "@types/chai": "4.1.7",
    "@types/lodash": "4.14.141",
    "babel-core": "6.26.3",
    "babel-loader": "8.0.6",
    "babel-preset-env": "1.7.0",
    "chai": "4.2.0",
    "dts-bundle-generator": "3.2.0",
    "mocha": "6.2.1",
    "raw-loader": "3.1.0",
    "rimraf": "3.0.0",
    "ts-loader": "6.2.0",
    "tslint": "5.20.0",
    "typedoc": "0.15.0",
    "typescript": "3.6.3",
    "uglify-js": "^3.5.11",
    "webpack": "4.41.0",
    "webpack-cli": "3.3.9",
    "webpack-node-externals": "1.7.2",
    "webpack-shell-plugin": "0.5.0"
  },
  "dependencies": {
    "lodash": "4.17.15"
  }
}
