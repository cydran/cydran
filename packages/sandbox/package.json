{
	"name": "@cydran/sandbox",
	"version": "0.5.0-snapshot",
	"description": "Cydran Sandbox",
	"main": "index.js",
	"private": true,
	"scripts": {
		"validate": "npx @cydran/build validate",
		"start": "npx webpack-dev-server --config=webpack.config.js --mode development --open",
		"build": "npx webpack --config=webpack.config.js  --mode production",
		"start.new": "npx @cydran/build start",
		"build.new": "npx @cydran/build build",
		"clean": "npx @cydran/build clean",
		"test": "npx @cydran/build test",
		"eject": "npx @cydran/build eject",
		"help": "npx @cydran/build help"
	},
	"author": "",
	"license": "ISC",
	"cydran-build": {
		"start": {
			"port": 8085
		},
		"eslint-rules": {
			"@typescript-eslint/no-explicit-any": "off",
			"@typescript-eslint/no-unused-vars": "off",
			"@typescript-eslint/ban-types": "off",
			"@typescript-eslint/no-this-alias": "off",
			"@typescript-eslint/no-unsafe-function-type": "off",
			"@typescript-eslint/no-wrapper-object-types": "off",
			"@typescript-eslint/no-empty-object-type": "off",
			"no-useless-escape": "off",
			"no-mixed-spaces-and-tabs": "off",
			"no-case-declarations": "off",
			"no-prototype-builtins": "off",
			"prefer-rest-params": "off",
			"no-fallthrough": "off",
			"prefer-spread": "off"
		}
	},
	"devDependencies": {
		"@babel/core": "7.25.2",
		"@cydran/build": "file:../build",
		"@jest/globals": "29.7.0",
		"@types/jest": "29.5.12",
		"animate.css": "4.1.1",
		"babel-core": "6.26.3",
		"babel-loader": "9.1.3",
		"babel-preset-env": "1.7.0",
		"contents-loader": "1.0.0",
		"copy-webpack-plugin": "12.0.2",
		"css-loader": "7.1.2",
		"file-loader": "6.2.0",
		"html-webpack-plugin": "5.6.0",
		"mini-css-extract-plugin": "2.9.1",
		"raw-loader": "4.0.2",
		"rimraf": "6.0.1",
		"sass": "1.78.0",
		"sass-loader": "16.0.1",
		"style-loader": "4.0.0",
		"ts-loader": "9.5.1",
		"typescript": "5.6.2",
		"webpack": "5.94.0",
		"webpack-cli": "5.1.4",
		"webpack-dev-server": "5.1.0"
	},
	"dependencies": {
		"@cydran/cydran": "file:../cydran",
		"@fortawesome/fontawesome-free": "6.6.0",
		"axios": "1.7.7",
		"bulma": "1.0.2",
		"markdown": "0.5.0",
		"navigo": "7.1.2"
	}
}
