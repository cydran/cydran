{
	"name": "@cydran/build",
	"version": "0.0.1",
	"description": "Simple Build Orchestration",
	"main": "index.js",
	"scripts": {
		"lerna.clean": "echo Not Yet Implemented",
		"lerna.build": "npm run build",
		"lerna.validate": "echo Not Yet Implemented",
		"lerna.test": "npm run test",
		"lerna.install": "npm install",
		"test": "test",
		"tschelp": "tsc -help",
		"build": "npm run prepare && tsc",
		"watch": "npm run prepare && tsc -w",
		"prepare": "touch bin/index.js && chmod +x bin/index.js"
	},
	"bin": {
		"@cydran/build": "bin/index.js"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/cydran/cydran.git"
	},
